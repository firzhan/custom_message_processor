    <com.example.guranteed-delivery.sequence_._deliveryFailSequence>capp/EnterpriseServiceBus</com.example.guranteed-delivery.sequence_._deliveryFailSequence>
    <com.example.guranteed-delivery.sequence_._deliverySequence>capp/EnterpriseServiceBus</com.example.guranteed-delivery.sequence_._deliverySequence>
    <com.example.guranteed-delivery.proxy-service_._GuranteedDeliveryProxy>capp/EnterpriseServiceBus</com.example.guranteed-delivery.proxy-service_._GuranteedDeliveryProxy>
    <com.example.guranteed-delivery.endpoint_._RealTimeEndPoint>capp/EnterpriseServiceBus</com.example.guranteed-delivery.endpoint_._RealTimeEndPoint>
    <com.example.guranteed-delivery.endpoint_._backupQueueEP>capp/EnterpriseServiceBus</com.example.guranteed-delivery.endpoint_._backupQueueEP>
    <com.example.guranteed-delivery.sequence_._moveSequence>capp/EnterpriseServiceBus</com.example.guranteed-delivery.sequence_._moveSequence>
  </properties>
  <dependencies>
    <dependency>
      <groupId>com.example.guranteed-delivery.sequence</groupId>
      <artifactId>deliverySequence</artifactId>
      <version>1.0.0</version>
      <type>xml</type>
    </dependency>
    <dependency>
      <groupId>com.example.guranteed-delivery.endpoint</groupId>
      <artifactId>RealTimeEndPoint</artifactId>
      <version>1.0.0</version>
      <type>xml</type>
    </dependency>
    <dependency>
      <groupId>com.example.guranteed-delivery.sequence</groupId>
      <artifactId>moveSequence</artifactId>
      <version>1.0.0</version>
      <type>xml</type>
    </dependency>
    <dependency>
      <groupId>com.example.guranteed-delivery.proxy-service</groupId>
      <artifactId>GuranteedDeliveryProxy</artifactId>
      <version>1.0.0</version>
      <type>xml</type>
    </dependency>
    <dependency>
      <groupId>com.example.guranteed-delivery.sequence</groupId>
      <artifactId>deliveryFailSequence</artifactId>
      <version>1.0.0</version>

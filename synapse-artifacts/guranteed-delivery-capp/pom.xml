      <groupId>com.example.guranteed-delivery.message-processors</groupId>
      <artifactId>ScheduledProcessor</artifactId>
      <version>1.0.0</version>
      <type>xml</type>
    </dependency>
    <dependency>
      <groupId>com.example.guranteed-delivery.endpoint</groupId>
      <artifactId>backupQueueEP</artifactId>
      <version>1.0.0</version>
      <type>xml</type>
    </dependency>
